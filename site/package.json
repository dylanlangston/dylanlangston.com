{
  "name": "dylanlangston.com",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test-bun": "bun --bun run --cwd ./ test:integration && bun --bun run --cwd ./ test:unit",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test --browser all",
    "test:unit": "vitest run"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.10.1",
    "@playwright/test": "^1.44.1",
    "@sveltejs/adapter-static": "^3.0.2",
    "@sveltejs/kit": "^2.5.17",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@types/eslint": "^8.56.7",
    "@types/node": "^20.14.6",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.12.0",
    "autoprefixer": "^10.4.19",
    "binaryen": "^117.0.0",
    "browser-dtector": "^4.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.40.0",
    "mdsvex": "^0.11.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.1",
    "svelte-feather-icons": "^4.1.0",
    "svelte-preprocess": "^6.0.1",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.4.1",
    "typescript": "^5.4.4",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  },
  "type": "module"
}
